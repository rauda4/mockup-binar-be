generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id       String @id @default(uuid())
  username String @unique
  email    String
  kodeunik String @unique
}

model todolist {
  id       String @id @default(uuid())
  todo    String
}
